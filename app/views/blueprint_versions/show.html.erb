    <%= render 'shared/show/association_table', record_object: @blueprint_version, association: :service_configurations, attributes: [:service_definition, :variables], fa: :'caret-right' %>
  <% end %>
  <% if @blueprint_version.software_version.software.framework.persistent_directories %>
