<div class="col-md-10 main-show-col-with-menu">
  <%= render 'shared/show/default', record_object: @blueprint_version, fa: :'file-text', back_path: @blueprint_version.software_version, attributes: 
        [ :name,
          :version,
          :memory,
          [:http_protocol, "HTTP protocol"],
          :framework_port_overide,
          :blocking_worker,
          :custom_start_script,
          :custom_install_script,
          :custom_post_install_script ]
  %>
