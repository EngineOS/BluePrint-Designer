<h1>Listing Licenses</h1>

<table>
 <tr>
  
   <th>Name</th>
   <th>URL</th>
   
   <th>Comment</th>
 </tr>
 <% @licenses.each do |license| %>
 <tr>
  <td><%= license.name %></td>
   <td><a href=
   	<%= license.sourceurl %>
   	>
   	<%= license.sourceurl %>
   	</a></td> 
  <td><%= license.comment %></td>
  <td><%= link_to 'Show', license_path(license) %></td>
    <td><%= link_to 'Edit', [:edit, license]   %></td>
   <td><%= link_to 'Remove', [  license],
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
 </tr>
 
 <% end %>
</table>

<%= link_to 'New License', new_license_path %>
